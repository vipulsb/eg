  "scripts": {
    "start:client": "webpack serve --config webpack.config.js",
    "build:client": "webpack --config webpack.config.js",
    "build:server": "nest build ",
    "clean": "rimraf dist",
    "build": "npm run clean && npm-run-all -p build:client build:server",
    "format": "prettier --write \"src/server/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test-client": "jest --config=jest.client.js",
    "test-server": "jest",
    "test": "npm-run-all -p test-client test-server",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
