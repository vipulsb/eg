    "@nestjs/serve-static": "^4.0.2",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express-session": "^1.18.1",
    "mongoose": "^8.8.2",
    "npm-run-all": "^4.1.5",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pino": "^9.5.0",
    "postcss-loader": "^8.1.1",
